{
  "name": "moon-syntax",
  "displayName": "moon-syntax",
  "description": "Syntax highlighting for Moon language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "moon",
      "aliases": ["Moon", "moon"],
      "extensions": [".mn"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "moon",
      "scopeName": "source.moon",
      "path": "./syntaxes/moon.tmLanguage.json"
    }],
		"themes": [{
			"label": "Moon Theme",
			"uiTheme": "vs-dark",
			"path": "./syntaxes/moon-theme.json"
		}]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Aram47/moon-syntax"
  }
}
